[program:jupyterhub]
command=jupyterhub -f {{ jupyterhub_config_dir }}/jupyterhub_config.py
redirect_stderr=true
stdout_logfile={{ jupyterhub_log_dir }}/jupyterhub.log
autostart=true
autorestart=false
stopasgroup=true
user=root
directory={{ jupyterhub_srv_dir }}
environment=PATH=%(ENV_PATH)s,SPARK_HOME=%(ENV_SPARK_HOME)s,PYSPARK_PYTHON=%(ENV_PYSPARK_PYTHON)s,PYSPARK_DRIVER_PYTHON=%(ENV_PYSPARK_DRIVER_PYTHON)s,PYTHONPATH=%(ENV_PYTHONPATH)s,SPARK_LOCAL_IP=%(ENV_SAPRK_LOCAL_IP)s
